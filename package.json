{
    "name": "@hdx/n8n-nodes-redis-streams-trigger",
    "version": "0.2.0",
    "description": "n8n trigger node for Redis Streams",
    "keywords": [
        "n8n-community-node-package"
    ],
    "license": "MIT",
    "homepage": "https://github.com/bl4ckbon3/hdx-n8n-nodes-redis-streams-trigger",
    "author": {
        "name": "David Megginson",
        "email": "megginson@un.org"
    },
    "main": "dist/nodes/RedisStreamsTrigger/RedisStreamsTrigger.node.js",
    "scripts": {
        "build": "tsc && cp src/redis.svg dist/",
        "dev": "tsc --watch",
        "format": "prettier --write nodes credentials",
        "lint": "eslint nodes credentials --ext .ts --fix",
        "lint:c": "eslint nodes credentials --ext .ts",
        "prepublishOnly": "npm run build"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nVersion": "^1.0.0",
        "credentials": [
            "dist/credentials/RedisCredentials.credentials.js"
        ],
        "nodes": [
            "dist/nodes/RedisStreamsTrigger/RedisStreamsTrigger.node.js"
        ]
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^18.18.0",
        "@typescript-eslint/parser": "^5.29.0",
        "eslint-plugin-n8n-nodes-base": "^1.11.0",
        "gulp": "^4.0.2",
        "n8n-core": "^1.0.0",
        "n8n-workflow": "^1.0.0",
        "prettier": "^2.7.1",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "redis": "^4.6.13"
    }
